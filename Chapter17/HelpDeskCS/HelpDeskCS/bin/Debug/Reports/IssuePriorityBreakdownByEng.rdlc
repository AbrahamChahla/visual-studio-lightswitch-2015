  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT Surname, Firstname, LowPriorityCount, MediumPriorityCount, HighPriorityCount  FROM
(SELECT Issue_Engineer , 
  SUM(LowPriority) AS LowPriorityCount, 
  SUM(MediumPriority) AS MediumPriorityCount, 
  SUM(HighPriority) AS HighPriorityCount FROM
		(SELECT 
		Issue_Engineer, 
		CASE WHEN Issue_Priority  = 1 THEN 1 ELSE 0 END AS LowPriority, 
		CASE WHEN Issue_Priority = 2 THEN 1 ELSE 0 END AS MediumPriority, 
		CASE WHEN Issue_Priority = 3 THEN 1 ELSE 0 END AS HighPriority 
		FROM 
		Issues 
		WHERE CreateDateTime &gt; DATEADD(mm, DATEDIFF(mm, 0, GETDATE()), 0)) RawData
GROUP BY Issue_Engineer ) SummedData
JOIN Engineers
ON SummedData.Issue_Engineer = Engineers.id</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Surname">
          <DataField>Surname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
