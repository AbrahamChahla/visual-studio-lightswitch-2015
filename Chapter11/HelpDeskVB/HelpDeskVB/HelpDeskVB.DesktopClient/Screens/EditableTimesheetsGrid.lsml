  <Application.Methods>
    <ApplicationMethod
      Name="ShowEditableTimesheetsGrid">
      <ApplicationMethod.Attributes>
        <ShowScreenMethod
          TargetScreen="EditableTimesheetsGrid" />
      </ApplicationMethod.Attributes>
    </ApplicationMethod>
  </Application.Methods>
</Application>